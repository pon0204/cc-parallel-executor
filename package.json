{
  "name": "claude-code-terminal",
  "version": "1.0.0",
  "description": "CC Parallel Execution System - Web-based terminal for Claude Code orchestration",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:all",
    "dev:all": "concurrently --names \"NEXT,SERVER,MCP\" --prefix-colors \"blue,green,yellow\" \"npm run dev:next\" \"npm run dev:server\" \"npm run dev:mcp\"",
    "dev:next": "next dev",
    "dev:server": "tsx watch server/index.ts",
    "dev:mcp": "cd mcp-server && npm run dev",
    "build": "npm run build:all",
    "build:all": "next build && tsc -p tsconfig.server.json && cd mcp-server && npm run build",
    "start": "npm run start:all",
    "start:all": "concurrently --names \"NEXT,SERVER,MCP\" --prefix-colors \"blue,green,yellow\" \"npm run start:next\" \"npm run start:server\" \"npm run start:mcp\"",
    "start:next": "next start",
    "start:server": "node dist/server/index.js",
    "start:mcp": "cd mcp-server && npm start",
    "install:all": "npm install && cd mcp-server && npm install",
    "clean": "rm -rf .next dist mcp-server/dist node_modules/.cache",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^5.16.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-select": "^2.1.10",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.51.1",
    "@tanstack/react-query-devtools": "^5.51.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "express": "^4.18.2",
    "lucide-react": "^0.513.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.55.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0",
    "winston": "^3.13.0",
    "yaml": "^2.4.5",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prisma": "^5.16.1",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.15.2",
    "typescript": "^5.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
