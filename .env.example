# Application Ports
NODE_ENV=development
NEXT_PORT=3000                    # Frontend (Next.js)
PROJECT_SERVER_PORT=3001          # Project Server (WebSocket + API)
MCP_SERVER_PORT=3002              # MCP Server (Streamable HTTP + SSE)

# Server URLs
PROJECT_SERVER_URL=http://localhost:3001
MCP_SERVER_URL=http://localhost:3002

# Database
DATABASE_URL="file:./dev.db"

# Claude Code
CLAUDE_PATH=/usr/local/bin/claude
MAX_PARALLEL_CC=5
CC_TIMEOUT_SECONDS=300

# Logging
LOG_LEVEL=info
LOG_DIR=./logs

# Git Worktree
GIT_WORKTREE_BASE_PATH=./worktrees

# MCP Configuration
MCP_SESSION_TIMEOUT=3600
MCP_SSE_ENABLED=true
ULTRATHINK_PROTOCOL_ENABLED=true